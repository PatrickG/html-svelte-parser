{
  "name": "html-svelte-parser",
  "description": "HTML to Svelte parser.",
  "version": "0.0.1",
  "license": "AGPL-3.0-only",
  "author": {
    "name": "Patrick GÃ¼nther",
    "url": "https://patrick.id/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PatrickG/html-svelte-parser.git"
  },
  "bugs": {
    "url": "https://github.com/PatrickG/html-svelte-parser/issues"
  },
  "keywords": [
    "svelte",
    "svelte-parser",
    "html-svelte-parser",
    "html-to-svelte"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "build:web": "vite build",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "playwright test"
  },
  "devDependencies": {
    "@playwright/experimental-ct-svelte": "^1.27.1",
    "@playwright/test": "^1.27.1",
    "@sveltejs/adapter-static": "^1.0.0-next.48",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "next",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-github-corner": "^0.1.0",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "type": "module",
  "dependencies": {
    "dom-serializer": "^2.0.0",
    "domhandler": "^5.0.3",
    "html-dom-parser": "^3.1.2",
    "htmlparser2": "^8.0.1"
  }
}
